{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "50mb",
          "memory": 1024,
          "timeout": 60
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      }
    ]
  }
  